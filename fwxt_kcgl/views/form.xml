<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <!--仓库设置-->
        <record id="view_form_warehouse_info" model="ir.ui.view">
            <field name="name">Warehouse Info</field>
            <field name="model">warehouse.info</field>
            <field name="arch" type="xml">
                <form string="Warehouse Info" version="7.0">
                    <sheet>
                        <h1>No.
                            <field name="code"/>
                        </h1>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="sf_default"/>
                                <field name="user_id"/>
                            </group>
                            <group>
                                <field name="messages"/>
                                <field name="date_confirm"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        
        <!--入库单据-->
        <record id="view_form_warehouse_doc" model="ir.ui.view">
            <field name="name">Warehouse Doc</field>
            <field name="model">warehouse.doc</field>
            <field name="arch" type="xml">
                <form string="Doc" version="7.0">
                <sheet>
                    <h1>No.
                        <field name="code" class="oe_inline"/>
                    </h1>
                    <group>
                        <group>
                            <field name="name" />
                            <field name="batch_id" />
                            <field name="commodity_id" />
                            <field name="unit_id" />
                            <field name="messages" />
                        </group>
                        <group>
                            <!--<field name="type"/>-->
                            <field name="warehouse_id"/>
                            <field name="number"/>
                            <field name="user_id" />
                            <field name="date_confirm" />
                        </group>
                    </group>
                    <notebook colspan="4">
                        <page string="瓶">
                            <field name="line_id"/>
                        </page>
                        <page string="箱">
                            <field name="two_id"/>
                        </page>
                        <page string="托">
                            <field name="one_id"/>
                        </page>
                    </notebook>
                </sheet>
                </form>
            </field>
        </record>

        <!--出库单据-->
        <record id="view_form_warehouse_doc_ck" model="ir.ui.view">
            <field name="name">Warehouse Doc</field>
            <field name="model">warehouse.doc</field>
            <field name="arch" type="xml">
                <form string="Doc" version="7.0">
                <sheet>
                    <h1>No.
                        <field name="code" class="oe_inline" />
                    </h1>
                    <group>
                        <group>
                            <field name="name" />
                            <field name="batch_id" />
                            <field name="commodity_id" />
                            <field name="unit_id" />
                            <field name="agent_id" />
                            <field name="messages" />
                        </group>
                        <group>
                            <!--<field name="type"/>-->
                            <field name="warehouse_id" />
                            <field name="number"/>
                            <field name="express_id"/>
                            <field name="express_code"/>
                            <field name="user_id" />
                            <field name="date_confirm" />
                        </group>
                    </group>
                    <notebook colspan="4">
                        <page string="瓶">
                            <field name="line_id"/>
                        </page>
                        <page string="箱">
                            <field name="two_id"/>
                        </page>
                        <page string="托">
                            <field name="one_id"/>
                        </page>
                    </notebook>
                </sheet>
                </form>
            </field>
        </record>

        <!--出入库明细-->
        <record id="view_form_warehouse_list" model="ir.ui.view">
            <field name="name">Warehouse Line</field>
            <field name="model">warehouse.line</field>
            <field name="arch" type="xml">
                <form string="Picture Management" version="7.0">
                <sheet>
                    <h1>No.
                        <field name="code" class="oe_inline" />
                    </h1>
                    <group>
                        <group>
                            <field name="name" />
                            <field name="number" />
                        </group>
                        <group>
                            <field name="user_id" />
                            <field name="date_confirm" />
                        </group>
                    </group>
                </sheet>
                </form>
            </field>
        </record>

        <!--条码明细表-->
        <record id="view_form_batch_list" model="ir.ui.view">
            <field name="name">Batch List</field>
            <field name="model">batch.list</field>
            <field name="arch" type="xml">
                <form string="Picture Management" version="7.0">
                <sheet>
                    <h1>No.
                        <field name="code" class="oe_inline" />
                    </h1>
                    <group>
                        <group>
                            <field name="name" />
                            <field name="messages" />
                        </group>
                        <group>
                            <field name="user_id" />
                            <field name="date_confirm" />
                        </group>
                    </group>
                </sheet>
                </form>
            </field>
        </record>

        <!--批量入库-->
        <record id="view_form_manual_storage" model="ir.ui.view">
            <field name="name">manual.storage.form</field>
            <field name="model">manual.storage</field>
            <field name="arch" type="xml">
                <form string="Manual Storage" version="7.0">
                    <h1>No.
                        <field name="code" class="oe_inline" />
                    </h1>
                    <group>
                        <group>
                            <field name="name" />
                            <field name="commodity_id" />
                            <field name="batch_id" />
                        </group>
                        <group>
                            <field name="warehouse_id" />
                            <field name="unit_id" />
                        </group>
                    </group>
                    <h1>出入库信息
                    </h1>
                    <group>
                        <group>
                            <field name="type"/>
                            <field name="express_id"/>
                        </group>
                        <group>
                            <field name="agent_id" />
                            <field name="express_code"/>
                        </group>
                    </group>
                    <group>
                        <group>
                            <field name="state_code"/>
                            <field name="user_id"/>
                        </group>
                        <group>
                            <field name="number" />
                            <field name="date_confirm"/>
                        </group>
                    </group>
                    <footer>
                        <button string="ubmiSt" name="manual_storage" type="object" default_focus="1" class="oe_highlight"/>
                        <!--<button string="Manual Storage" class="oe_link" special="cancel"/>-->
                        or
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

    </data>
</openerp>
