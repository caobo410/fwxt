<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <!--仓库设置-->
        <record id="view_form_warehouse_info" model="ir.ui.view">
            <field name="name">Warehouse Info</field>
            <field name="model">warehouse.info</field>
            <field name="arch" type="xml">
                <form string="Warehouse Info" version="7.0">
                    <sheet>
                        <h1>No.
                            <field name="code"/>
                        </h1>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="sf_default"/>
                                <field name="user_id"/>
                            </group>
                            <group>
                                <field name="messages"/>
                                <field name="date_confirm"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        
        <!--入库单据-->
        <record id="view_form_warehouse_doc" model="ir.ui.view">
            <field name="name">Warehouse Doc</field>
            <field name="model">warehouse.doc</field>
            <field name="arch" type="xml">
                <form string="Doc" version="7.0">
                <sheet>
                    <h1>No.
                        <field name="code" class="oe_inline" />
                    </h1>
                    <group>
                        <group>
                            <field name="name" />
                            <field name="batch_id" />
                            <field name="commodity_id" />
                            <field name="unit_id" />
                            <field name="messages" />
                        </group>
                        <group>
                            <field name="type" eval="in"/>
                            <field name="warehouse_id" />
                            <field name="number"/>
                            <field name="user_id" />
                            <field name="date_confirm" />
                        </group>
                    </group>
                    <notebook colspan="4">
                        <page string="Batch">
                            <field name="batch_list"/>
                        </page>
                    </notebook>
                </sheet>
                </form>
            </field>
        </record>
        
